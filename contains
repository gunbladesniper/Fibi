var contains = function(array, int){
  var confirm = false;
  array.forEach(function(elem, i){
    if(elem === int){
      confirm = true;
    }

  })
  return confirm;
}
console.log(contains([1, 5, 6, 3], 3));// => true
console.log(contains([3, 5, 6, 1], 3));// => true
console.log(contains([7, 1, 6, 4], 3));// => false
