
filter([1, 2, 3, 4, 5], function(elem, i) {
  return elem % 2 === 0;
}) // => [2, 4]

contains([1, 5, 6. 3], 3)// => true

any([1, 4, 6, 7, 4], function(elem, i) {
  return elem % 2 === 0;
}) // return true

every([1, 4, 6, 7, 4], function(elem, i) {
  return elem % 2 === 0;
}) // return false
